<div>
    <form>
        <div>
            <div>
                <select id="left-valute">
                    {{#each Valute}}
                    <option value="{{Value}}">{{Name}}</option>
                    {{/each}}
                </select>
                <input id="left-text" value="0" placeholder="100">
            </div>

            <div>
                <select id="right-valute">
                    {{#each Valute}}
                        <option value="{{Value}}">{{Name}}</option>
                    {{/each}}
                </select>
                <input id="right-text" value="0" placeholder="100">
            </div>
        </div>
    </form>
</div>

<div>
    <table>
        <thead>
            <tr>
                <th>#</th>
                <th>Название</th>
                <th>Обозначение</th>
                <th>Курс к рублю</th>
                <th>Рубль к валюте</th>
            </tr>
        </thead>
        <tbody>
            {{#each Valute}}
                <tr>
                    <td>#</td>
                    <td>{{Name}}</td>
                    <td>{{CharCode}}</td>
                    <td>{{Value}}</td>
                    <td>{{DeValue}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>